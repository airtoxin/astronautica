  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "@types/prettier": "2.4.2",
    "express": "4.17.2",
    "jest": "27.4.5",
    "npm-run-all": "4.1.5",
    "ts-jest": "27.1.1",
    "typescript": "4.5.3",
    "wait-on": "6.0.0"
  }
}
