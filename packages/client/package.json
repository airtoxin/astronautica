    "dev:ts": "tsc --watch",
    "prebuild": "yarn codegen",
    "build": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "3.5.6",
    "undici": "5.8.0",
    "prettier": "2.5.1"
